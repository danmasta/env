{
  "name": "@danmasta/env",
  "private": true,
  "version": "5.2.0",
  "author": "Daniel Smith <dannmasta@gmail.com>",
  "description": "Environment helper for node apps",
  "license": "MIT",
  "keywords": [
    "config",
    "dotenv",
    "env",
    "environment"
  ],
  "main": "index.js",
  "type": "module",
  "types": "index.d.ts",
  "files": [
    "index.js",
    "dist/**/*",
    "lib/**/*",
    "**/*.d.ts"
  ],
  "exports": {
    ".": {
      "import": "./index.js",
      "require": "./dist/cjs/index.cjs"
    },
    "./env": {
      "import": "./lib/env.js",
      "require": "./dist/cjs/lib/env.cjs"
    }
  },
  "scripts": {
    "test": "make test",
    "coverage": "make coverage",
    "build": "make build",
    "watch": "make watch",
    "preversion": "make build test status"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/danmasta/env.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "dependencies": {
    "lo": "github:danmasta/lo#semver:0.0.2"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-replace": "^6.0.2",
    "c8": "^10.1.3",
    "chai": "^5.2.1",
    "eslint": "^9.32.0",
    "mocha": "^11.7.1",
    "rollup": "^4.46.2"
  }
}
